{% extends 'base.html.twig' %}
{% block stylesheets %}
<style>
label {
  width: 100px;
  display: inline-block;
  border-bottom: solid 1px;
}
li {
  
  list-style-type: none;
}
</style>
{% endblock %}
{% block body %}
    <h2>Erstelle einen Kontakteintrag</h2>    
    {{ form_start(form) }}
    <fieldset>
        <legend>Bitte Kontaktperson eingeben</legend>
        <table>
            <tr>
                <td>{{ form_label(form.ansprechpartner, 'Ansprechpartner') }}</td>
                <td>{{ form_widget(form.ansprechpartner) }}</td>
                <td>{{ form_errors(form.ansprechpartner) }}</td>
            </tr>
        </table>
        <input type="submit" value="speichern">
    </fieldset>
    {{ form_errors(form) }}
    {{ form_end(form) }}
    
    <br>Zur√ºck zur <a href="{{ path('homepage') }}">Hauptseite</a>
{% endblock %}